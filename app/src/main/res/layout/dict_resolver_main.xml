<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <EditText
        android:id="@+id/dictResolver_word"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="word"/>
    <EditText
        android:id="@+id/dictResolver_detail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="detail"/>
    <Button
        android:id="@+id/dictResolver_insert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="insert"/>
    <EditText
        android:id="@+id/dictResolver_key"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="key"
        android:layout_marginTop="20dp"/>
    <Button
        android:id="@+id/dictResolver_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="search"/>

</LinearLayout>
